{
  "name": "picshare",
  "version": "1.0.0",
  "description": "MERN Stack with Typescript picShare",
  "main": "server.ts",
  "scripts": {
    "server": "nodemon --watch server --exec ts-node -- ./server/server.ts",
    "client": "cd client && npm start",
    "start": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thebilz2/mern-picShare.git"
  },
  "keywords": [
    "MERN",
    "Stack",
    "Typescript",
    "picShare"
  ],
  "author": "Bilal Azam",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thebilz2/mern-picShare/issues"
  },
  "homepage": "https://github.com/thebilz2/mern-picShare#readme",
  "dependencies": {
    "body-parser": "^1.20.2",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^7.0.1",
    "nodemon": "^2.0.21",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/mongoose": "^5.11.97"
  }
}
